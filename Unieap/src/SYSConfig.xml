<?xml version="1.0" encoding="UTF-8"?>
<sysConfig>
	<!-- true ,false -->
	<isDebug>false</isDebug>
	<!--业务操作日志 true ,false -->
	<isBizLog>false</isBizLog>
	<!--数据操作日志 true ,false -->
	<isDataLog>false</isDataLog>
	<!-- default language zh_CN,en_US -->
	<defaultLanguage>zh_CN</defaultLanguage>
	<!--查询配置文件路径 WEB-INF/conf/unieap/query-->
	<queryPath>/WEB-INF/conf/unieap/query,/WEB-INF/conf/apps/mdm/query,/WEB-INF/conf/apps/yzs/query</queryPath>
	<!--工作流配置文件路径 WEB-INF/conf/unieap/workflow-->
	<wfPath>/WEB-INF/conf/unieap/workflow</wfPath>
	<!-- 表报配置路径 -->
	<rpPath>/WEB-INF/conf/unieap/report</rpPath>
	<!--附件存放路径 -->
	<filePath>/apps/mantis/attachment</filePath>
	<rootPath>C:/workspace/myeclipse</rootPath>
	<!--MYSQL,ORACLE-->
	<dbType>MYSQL</dbType>
	<!-- 采用isHibernate持久化-->
	<isHibernate>true</isHibernate>
	<!--一次性查询最多纪录数 -->
	<selectMaxNum>100000</selectMaxNum>
	<!-- 默认分页 -->
	<pageSize>15</pageSize>
</sysConfig>