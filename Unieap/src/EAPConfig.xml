<?xml version="1.0" encoding="UTF-8"?>
<eapConfig>
	<!--请用插件时,请起用applicationContext.xml中的applicationContext-handler.xml配置文件  -->
	<!--初始化快码数据-->
	<component name="dicDataHandler" isEnabled="true"
		handler="dicDataHandler" description="初始化快码数据">
	</component>
	<!--初始化需要验证的资源数据-->
	<component name="securityHandler" isEnabled="false"
		handler="securityHandler" description="初始化需要验证的资源数据">
	</component>
	<!--缓存菜单数据-->
	<component name="menuHandler" isEnabled="false"
		handler="menuHandler" description="缓存菜单数据">
	</component>
	<!--缓存按钮数据-->
	<component name="buttonHandler" isEnabled="false"
		handler="buttonHandler" description="缓存按钮数据">
	</component>
	<!--启动流程引擎,请请用applicationContext.xml中的applicationContext-jbpm.xml配置文件-->
	<component name="processEngineHandler" isEnabled="false"
		handler="processEngineHandler" description="启动流程引擎">
	</component>
	<!--发布流程模板,必须是在启动了流程引擎才能发布模板,开启该功能时,请起用applicationContext.xml中的applicationContext-jbpm.xml配置文件-->
	<component name="jbpmDeployHandler" isEnabled="false"
		handler="jbpmDeployHandler" description="发布流程模板">
	</component>
	<!--表报解析,加载到内存中-->
	<component name="reportHandler" isEnabled="false"
		handler="reportHandler" description="表报解析,加载到内存中">
	</component>
</eapConfig>